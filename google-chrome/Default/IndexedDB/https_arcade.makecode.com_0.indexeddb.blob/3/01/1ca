forever(function () {
    if (mySprite.tileKindAt(TileDirection.Center, assets.tile`tile5`) && info.score() > 499) {
        info.changeScoreBy(-500)
        game.over(true)
    }
})
forever(function () {
    mySprite.vy = 0
    mySprite.vx = 0
    if (controller.up.isPressed()) {
        mySprite.vx = _xx * speed
        mySprite.vy = _yy * speed
    }
    if (controller.down.isPressed()) {
        mySprite.vx = _xx * (0 - speed)
        mySprite.vy = _yy * (0 - speed)
    }
    if (controller.left.isPressed()) {
        transformSprites.changeRotation(mySprite, -5)
        getAngle()
    }
    if (controller.right.isPressed()) {
        transformSprites.changeRotation(mySprite, 5)
        getAngle()
    }
})
forever(function () {
    myEnemy = sprites.create(img`
        .........................
        ..........fffffff........
        .........f7777777f.......
        ........f777777777f......
