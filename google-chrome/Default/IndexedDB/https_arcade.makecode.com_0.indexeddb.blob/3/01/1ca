<value name="B"><shadow type="math_number" id="/K;q`67Pe5I!i6IsixE%"><field name="NUM">180</field></shadow></value></block></value><next><block type="variables_set" id=",-iWPxM:%T,X@YDsfK3+"><field name="VAR" id="^0)%.cCPbm*kr?(blz(a">_xx</field><value name="VALUE"><shadow type="math_number"><field name="NUM">0</field></shadow><block type="math_js_op" id="oW7@S5GI,I%dnv%s4tGq"><mutation xmlns="http://www.w3.org/1999/xhtml" op-type="unary"></mutation><field name="OP">cos</field><value name="ARG0"><shadow type="math_number"><field name="NUM">0</field></shadow><block type="variables_get" id="*HR0$,!ofW5h+^+1LA]E"><field name="VAR" id="mU?Z*ubXZU*{esgG5JYr">radAngle</field></block></value></block></value><next><block type="variables_set" id="VHN`_N$zhd/t(K;{TSLU"><field name="VAR" id="ii%pHY=EW}-CI][[ck1d">_yy</field><value name="VALUE"><shadow type="math_number"><field name="NUM">0</field></shadow><block type="math_js_op" id="yH2%pTOQS0B8%0u5{!Zb"><mutation xmlns="http://www.w3.org/1999/xhtml" op-type="unary"></mutation><field name="OP">sin</field><value name="ARG0"><shadow type="math_number"><field name="NUM">0</field></shadow><block type="variables_get" id="H!iW4I]S-hry(`n5`u6m"><field name="VAR" id="mU?Z*ubXZU*{esgG5JYr">radAngle</field></block></value></block></value></block></next></block></next></block></statement></block><block type="device_pause" id="F0i?yKNzbQ)p?~{;o@bs" disabled="true" x="0" y="2555"><value name="pause"><shadow type="timePicker" id="lH#9[bmk9]6#~iU31^JT" disabled="true"><field name="ms">100</field></shadow></value></block></xml>"main.ts"ïýnamespace SpriteKind {
    export const enemy2 = SpriteKind.create()
    export const powerup = SpriteKind.create()
    export const enemy3 = SpriteKind.create()
    export const ENEMYLAVA = SpriteKind.create()
    export const Runner = SpriteKind.create()
    export const Boss = SpriteKind.create()
    export const UltraProjectile = SpriteKind.create()
    export const ForestEnemy = SpriteKind.create()
    export const ENEMYPROJECTILE = SpriteKind.create()
}
sprites.onOverlap(SpriteKind.Projectile, SpriteKind.Runner, function (sprite, otherSprite) {
    otherSprite.destroy()
    info.changeScoreBy(5)
})
sprites.onOverlap(SpriteKind.Player, SpriteKind.ForestEnemy, function (sprite, otherSprite) {
    info.changeLifeBy(-1)
    pause(1000)
})
controller.B.onEvent(ControllerButtonEvent.Pressed, function () {
    if (info.score() > 199) {
        SuperProjectile = sprites.create(img`
            . . . . . 
            . 9 9 9 9 
            . 9 9 9 9 
            . 9 9 9 9 
            `, SpriteKind.UltraProjectile)
        SuperProjectile.setPosition(mySprite.x, mySprite.y)
        SuperProjectile.setVelocity(_xx * 255, _yy * 255)
        info.changeScoreBy(-200)
        pause(200)
    } else {
        info.changeScoreBy(0)
    }
})
sprites.onOverlap(SpriteKind.Projectile, SpriteKind.ENEMYLAVA, function (sprite, otherSprite) {
    otherSprite.destroy()
    info.changeScoreBy(5)
})
controller.A.onEvent(ControllerButtonEvent.Pressed, function () {
    projectile = sprites.createProjectileFromSprite(img`
        5 5 
        `, mySprite, _xx * 225, _yy * 225)
    pause(100)
})
sprites.onOverlap(SpriteKind.Player, SpriteKind.Runner, function (sprite, otherSprite) {
    info.changeLifeBy(-1)
    pause(1000)
})
sprites.onOverlap(SpriteKind.Player, SpriteKind.ENEMYLAVA, function (sprite, otherSprite) {
    info.changeLifeBy(-1)
    pause(1000)
})
function getAngle () {
    radAngle = transformSprites.getRotation(mySprite) * Math.PI / 180
    _xx = Math.cos(radAngle)
    _yy = Math.sin(radAngle)
}
sprites.onOverlap(SpriteKind.Player, SpriteKind.enemy2, function (sprite, otherSprite) {
    info.changeLifeBy(-1)
    pause(1000)
})
sprites.onOverlap(SpriteKind.UltraProjectile, SpriteKind.Boss, function (sprite, otherSprite) {
    otherSprite.destroy(effects.fire, 200)
    info.changeScoreBy(50)
})
sprites.onOverlap(SpriteKind.Projectile, SpriteKind.enemy3, function (sprite, otherSprite) {
    otherSprite.destroy()
    info.changeScoreBy(3)
})
sprites.onOverlap(SpriteKind.Projectile, SpriteKind.enemy2, function (sprite, otherSprite) {
    otherSprite.destroy()
    MyEnemy2 = sprites.create(img`
        .........................
        ..........fffffff........
        .........f2444422f.......
        ........f224444444f......
        .......f22444444442f.....
        .......f44444422242f.....
        .......f44244422224f.....
        .......f42244442224f.....
        .......f44444444244f.....
        .......f24444444442f.....
        .......f44222444442f.....
        ........f442244422f......
        .........f2444442f.......
        ..........fffffff........
        .........................
        .........................
        `, SpriteKind.enemy3)
    tiles.placeOnRandomTile(MyEnemy2, sprites.dungeon.hazardHole)
    MyEnemy2.follow(mySprite, 10)
    info.changeScoreBy(1)
})
sprites.onOverlap(SpriteKind.Projectile, SpriteKind.ForestEnemy, function (sprite, otherSprite) {
    otherSprite.destroy()
    info.changeScoreBy(15)
})
sprites.onOverlap(SpriteKind.Player, SpriteKind.enemy3, function (sprite, otherSprite) {
    info.changeLifeBy(-1)
    pause(1000)
})
sprites.onOverlap(SpriteKind.Boss, SpriteKind.Projectile, function (sprite, otherSprite) {
    otherSprite.destroy()
})
sprites.onOverlap(SpriteKind.Projectile, SpriteKind.Enemy, function (sprite, otherSprite) {
    otherSprite.destroy()
    info.changeScoreBy(1)
})
sprites.onOverlap(SpriteKind.Player, SpriteKind.Enemy, function (sprite, otherSprite) {
    info.changeLifeBy(-1)
    pause(1000)
})
sprites.onOverlap(SpriteKind.Player, SpriteKind.Boss, function (sprite, otherSprite) {
    info.changeLifeBy(-1)
    pause(1000)
})
let big_boy_ben: Sprite = null
let myEnemy: Sprite = null
let Forest_Zombie: Sprite = null
let Lava_Enemy: Sprite = null
let Massive_boy_ben: Sprite = null
let MyEnemy2: Sprite = null
let radAngle = 0
let projectile: Sprite = null
let _yy = 0
let SuperProjectile: Sprite = null
let mySprite: Sprite = null
let _xx = 0
_xx = 1
let speed = 100
let bspeed = 50
