{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "shared/popover/images/okta-32x32.png",
      "default_popup": "shared/popover/popover.html"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "default": "Alt+O",
            "mac": "Command+Shift+O",
            "windows": "Alt+O"
         }
      }
   },
   "content_security_policy": "object-src 'self';script-src 'self';img-src https://*.oktacdn.com https://system.okta.com https://*.cloudareanet.com https://*.clouditude.com https://*.hioktane.com https://*.lowoktane.com https://*.openalloy.com https://*.okta.com https://*.okta1.com https://*.okta-emea.com https://*.oktapreview.com https://*.perfcloud.org https://*.primeapp.com https://*.trexcloud.com https://*.widerock.com https://*.okta.ninja https://*.okta.mil https://*.okta-miltest.com https://*.okta-gov.com https://*.trex-govcloud.com;style-src 'unsafe-inline' https://*.oktacdn.com",
   "description": "Work outside the perimeter",
   "icons": {
      "128": "img/edge-chromium-store-app-icon-128.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Okta Browser Plugin",
   "optional_permissions": [ "privacy" ],
   "options_ui": {
      "browser_style": false,
      "open_in_tab": true,
      "page": "shared/settings-page/SettingsPage.html"
   },
   "permissions": [ "tabs", "cookies", "https://*/", "http://*/", "management", "storage", "unlimitedStorage", "webRequest", "webRequestBlocking", "webNavigation" ],
   "update_url": "https://edge.microsoft.com/extensionwebstorebase/v1/crx",
   "version": "6.13.0",
   "web_accessible_resources": [ "getFrameId" ]
}
